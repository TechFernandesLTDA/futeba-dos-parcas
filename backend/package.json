{
  "name": "futeba-dos-parcas-backend",
  "version": "1.0.0",
  "description": "Backend API para o aplicativo Futeba dos Parcas",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- migration:generate -d src/config/database.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/config/database.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/config/database.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "typeorm": "^0.3.19",
    "reflect-metadata": "^0.2.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "express-validator": "^7.0.1",
    "ws": "^8.16.0",
    "firebase-admin": "^12.0.0",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "moment": "^2.30.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/ws": "^8.5.10",
    "@types/multer": "^1.4.11",
    "@types/uuid": "^9.0.7",
    "@types/node-cron": "^3.0.11",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0"
  },
  "keywords": [
    "futeba",
    "futebol",
    "pelada",
    "backend",
    "api"
  ],
  "author": "",
  "license": "ISC"
}
